{
  "name": "seqcode-vscode",
  "displayName": "SeqCode",
  "description": "SeqCode language support",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/belteshazzar/seqcode-vscode"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "seqcode",
      "aliases": ["SeqCode", "seqcode"],
      "extensions": [".seqcode"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "seqcode",
      "scopeName": "source.seqcode",
      "path": "./syntaxes/seqcode.tmLanguage.json"
    }]
  }
}
